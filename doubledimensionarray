public class doubledimensionarray {
    public static void main(String args[]){
        int[][] arr=new int[][] {{3,4},{6,7},{9,10}};
        int i,j;
        for(i=0;i<3;i++){
            for(j=0;j<2;j++){
                System.out.print(arr[i][j]+" ");
            }
            System.out.println();
        }
    }
}